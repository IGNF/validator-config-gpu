{
    "id": "c574d390f93dc8f0ff0ec0496dc995cc",
    "name": "DOC_URBA",
    "title": "DOC_URBA",
    "description": "Table contenant la liste des documents d'Urbanisme PLU ou POS dont la numérisation ou l'élaboration sous fourme numérique est engagée",
    "columns": [
        {
            "type": "Url",
            "id": "099853b5a7d6d113dc6249644e362888",
            "name": "URLPLAN",
            "title": "URLPLAN",
            "description": "URL ou URI qui pointe sur le fichier du plan scanné",
            "constraints": {
                "required": false,
                "unique": false,
                "maxLength": 254
            }
        },
        {
            "type": "String",
            "id": "205a8d0f0ea01b09644b118b62deddad",
            "name": "NOMREG",
            "title": "NOMREG",
            "description": "Nom du fichier contentant le document complet du règlement",
            "constraints": {
                "required": false,
                "unique": false,
                "maxLength": 80
            }
        },
        {
            "type": "Url",
            "id": "43c2fa308b1a3991ed44edf5c17c2699",
            "name": "URLREG",
            "title": "URLREG",
            "description": "URL ou URI qui pointe sur le fichier du règlement papier complet scanné",
            "constraints": {
                "required": false,
                "unique": false,
                "maxLength": 254
            }
        },
        {
            "type": "String",
            "id": "5096b01df29475bfe6cfd5e546eb92ae",
            "name": "IDURBA",
            "title": "IDURBA",
            "description": "Identifiant du document d'urbanisme",
            "constraints": {
                "required": true,
                "unique": false,
                "maxLength": 20
            }
        },
        {
            "type": "String",
            "id": "6aeaa4493cb14c7acc2a8b1d5fc232ac",
            "name": "SIREN",
            "title": "SIREN",
            "description": "Indique le numéro SIREN de l'intercommunalité ayant approuvé le PLU. Clé externe permettant de faire le lien avec la table des EPCI",
            "constraints": {
                "required": false,
                "unique": false,
                "maxLength": 9
            }
        },
        {
            "type": "String",
            "id": "8323933b4b9ea2607c4c5d00b1a8e989",
            "name": "SITEWEB",
            "title": "SITEWEB",
            "description": "Page web du service de consultation du PLU offert par la collectivité locale",
            "constraints": {
                "required": false,
                "unique": false,
                "maxLength": 254
            }
        },
        {
            "type": "String",
            "id": "87c4fe0f555b9e1bfaf3652742e94a27",
            "name": "DATEFIN",
            "title": "DATEFIN",
            "description": "Date de fin de validité du document d'urbanisme",
            "constraints": {
                "required": false,
                "unique": false,
                "pattern": "[0-9]{8}"
            }
        },
        {
            "type": "String",
            "id": "8bd63266ee0014d105b9d5d6c2d7725a",
            "name": "NOMPLAN",
            "title": "NOMPLAN",
            "description": "Nom du fichier contenant le plan origine scanné. Il s'agit du plan sur lequel figure le tampon juridique",
            "constraints": {
                "required": false,
                "unique": false,
                "maxLength": 80
            }
        },
        {
            "type": "String",
            "id": "d26ce301c5af1306e1795b0bd2caf5b9",
            "name": "DATEREF",
            "title": "DATEREF",
            "description": "Date d'actualité ou de mise à jour du référentiel cadastral utilisé pour la création ou la numérisation des documents graphiques.",
            "constraints": {
                "required": false,
                "unique": false,
                "pattern": "[0-9]{8}"
            }
        },
        {
            "type": "String",
            "id": "d4e2e92e85c26da2802b498b54ed0e92",
            "name": "TYPEREF",
            "title": "TYPEREF",
            "description": "Nom du référentiel cadastral utilisé pour la numérisation",
            "constraints": {
                "required": false,
                "unique": false,
                "enum": [
                    "01",
                    "02"
                ],
                "maxLength": 2
            }
        },
        {
            "type": "String",
            "id": "dfa04d9c1e2925bae5438d3c1e655c0d",
            "name": "TYPEDOC",
            "title": "TYPEDOC",
            "description": "Type du document",
            "constraints": {
                "required": true,
                "unique": false,
                "enum": [
                    "PLU",
                    "POS",
                    "CC",
                    "PSMV"
                ],
                "maxLength": 4
            }
        },
        {
            "type": "String",
            "id": "e51b7a9fb363369cc5fd0d2c797e8b37",
            "name": "ETAT",
            "title": "ETAT",
            "description": "Etat juridique du document d'urbanisme",
            "constraints": {
                "required": true,
                "unique": false,
                "enum": [
                    "01",
                    "03",
                    "04",
                    "05",
                    "06",
                    "07"
                ],
                "maxLength": 2
            }
        },
        {
            "type": "String",
            "id": "ef6fad7808a7155f595334b7a63a83ad",
            "name": "DATAPPRO",
            "title": "DATAPPRO",
            "description": "Date de la dernière approbation administrative du document d'urbanisme",
            "constraints": {
                "required": false,
                "unique": false,
                "pattern": "[0-9]{8}"
            }
        },
        {
            "type": "String",
            "id": "f6f42ffb71252d1df5a2204c9ea17fdd",
            "name": "INTERCO",
            "title": "INTERCO",
            "description": "Indique si le document d'urbanisme est intercommunal",
            "constraints": {
                "required": true,
                "unique": false,
                "maxLength": 1,
                "reference": "ListeInterco(INTERCO)"
            }
        }
    ],
    "constraints": {
        "conditions": [
            "DATAPPRO IS NOT NULL OR ETAT NOT LIKE '03'",
            "DATEFIN IS NOT NULL OR ETAT NOT IN ('04', '05')",
            "INTERCO LIKE 'F'"
        ],
        "foreignKeys": [
            "(INTERCO) REFERENCES ListeInterco(INTERCO)"
        ]
    }
}