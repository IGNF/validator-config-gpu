{
    "id": "825cf62fb28f56c3f1a690fd3c5f48f3",
    "name": "INFO_PCT",
    "title": "INFO_PCT",
    "description": "Table contenant les informations ponctuelle à reporter à titre d'information",
    "constraints": {
        "conditions": [
            "NOMFIC IS NOT NULL OR TYPEINF NOT LIKE '97'"
        ],
        "foreignKeys": [
            "(TYPEINF, STYPEINF) REFERENCES InformationUrbaType(TYPEINF, STYPEINF)"
        ]
    },
    "columns": [
        {
            "id": "08c9b86ba6bd3a4178b5bfbf0b564d58",
            "name": "LIB_IDINFO",
            "title": "LIB_IDINFO",
            "description": "identifiant d'objets",
            "type": "String",
            "constraints": {
                "required": false,
                "unique": false,
                "pattern": "[AB0-9]{5}_PLU_[0-9]{8}_IP[0-9]+",
                "maxLength": 40
            }
        },
        {
            "id": "4257d4ea8b1be8111f9c44aed59d12f7",
            "name": "IDURBA",
            "title": "IDURBA",
            "description": "Identifiant du document d'urbanisme",
            "type": "String",
            "constraints": {
                "required": true,
                "unique": false,
                "maxLength": 30
            }
        },
        {
