{
    "id": "d9e315c3de1de6f99683235d3ebc144a",
    "name": "INFO_PCT",
    "title": "INFO_PCT",
    "description": "Table contenant les informations ponctuelle à reporter à titre d'information",
    "columns": [
        {
            "id": "2f3146a9f28a85044005b6876bb372d4",
            "name": "NOMFIC",
            "title": "NOMFIC",
            "description": "Nom du fichier contenant le texte décrivant l'information reportée",
            "type": "Filename",
            "constraints": {
                "required": false,
                "unique": false,
                "maxLength": 80
            }
        },
        {
            "id": "35b4011c916012f85fa4b116e1ae8cd1",
            "name": "WKT",
            "title": "Géométrie",
            "description": "Geométrie associée",
            "type": "MultiPoint",
            "constraints": {
                "required": true,
                "unique": false
            }
        },
        {
            "id": "5d3a21d212f8bd48cee06951a1b3d94b",
            "name": "IDINFO",
            "title": "IDINFO",
            "description": "Identifiant unique de l’objet",
            "type": "String",
            "constraints": {
                "required": false,
                "unique": false,
                "pattern": "POIN([0-9]+)?",
                "maxLength": 10
            }
        },
        {
            "id": "6471b663e6b10205b4859596e1a01842",
            "name": "URLFIC",
            "title": "URLFIC",
            "description": "URL ou URI qui pointe sur le fichier de règlement de l'information",
            "type": "Url",
            "constraints": {
                "required": false,
                "unique": false,
                "maxLength": 254
            }
        },
        {
            "id": "9423fc2ca70a794061a148ee43f10193",
            "name": "TYPEINF",
            "title": "TYPEINF",
            "description": "Type d'information ponctuelle dans une liste prédéfinie",
            "type": "String",
            "constraints": {
                "required": true,
                "unique": false,
                "enum": [
                    "16",
                    "19",
                    "27",
                    "99"
                ],
                "maxLength": 2
            }
        },
        {
            "id": "d689ae2f6c4549f59377f9837f19009c",
            "name": "LIBELLE",
            "title": "LIBELLE",
            "description": "Nom complet de l'information",
            "type": "String",
            "constraints": {
                "required": true,
                "unique": false,
                "maxLength": 254
            }
        },
        {
            "id": "ddbbad48547a3328d4cb769b16cc0c8f",
            "name": "TXT",
            "title": "TXT",
            "description": "Nom court de l'information servant à faire une étiquette",
            "type": "String",
            "constraints": {
                "required": false,
                "unique": false,
                "maxLength": 10
            }
        },
        {
            "id": "de4c2bf4b0e71abf278086afc2f37866",
            "name": "INSEE",
            "title": "INSEE",
            "description": "Code INSEE de la commune concernée par l'information",
            "type": "String",
            "constraints": {
                "required": true,
                "unique": false,
                "maxLength": 5
            }
        }
    ]
}