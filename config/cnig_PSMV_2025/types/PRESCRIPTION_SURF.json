{
    "id": "df55a00ab7e705d08baade2ae89d9200",
    "name": "PRESCRIPTION_SURF",
    "title": "PRESCRIPTION_SURF",
    "description": "Table contenant les prescriptions surfaciques se superposant au document d'urbanisme et le lien avec le texte qui la décrit",
    "columns": [
        {
            "type": "Filename",
            "id": "023007d7b302e7b6fa9689b01891498b",
            "name": "NOMFIC",
            "title": "NOMFIC",
            "description": "Nom du fichier PDF décrivant la prescription",
            "constraints": {
                "required": false,
                "unique": false,
                "maxLength": 80
            }
        },
        {
            "type": "String",
            "id": "2690113206632d0b5c93284ce371c073",
            "name": "SYMBOLE",
            "title": "SYMBOLE",
            "description": "SYMBOLE",
            "constraints": {
                "required": false,
                "unique": false,
                "pattern": "PSC-S_<ENTIER_2>-<ENTIER_2>_<ENTIER_3>",
                "maxLength": 20
            }
        },
        {
            "type": "String",
            "id": "43457d430b223d5342f06864d6ad6f39",
            "name": "IDURBA",
            "title": "IDURBA",
            "description": "Identifiant du document d'urbanisme",
            "constraints": {
                "required": true,
                "unique": false,
                "maxLength": 30
            }
        },
        {
            "type": "String",
            "id": "46efe25efd886390aa4c9aec76385f55",
            "name": "STYPEPSC",
            "title": "STYPEPSC",
            "constraints": {
                "required": true,
                "unique": false,
                "maxLength": 2
            }
        },
        {
            "type": "String",
            "id": "603dea3791f747f7a9d0ac0b613b1925",
            "name": "DATVALID",
            "title": "DATVALID",
            "description": "Date de la dernière validation de la prescription",
            "constraints": {
                "required": false,
                "unique": false,
                "pattern": "[0-9]{8}"
            }
        },
        {
            "type": "MultiPolygon",
            "id": "72b1e9a11d3079bc3cd6f439c58a2824",
            "name": "WKT",
            "title": "Géométrie",
            "description": "Géométrie associée",
            "constraints": {
                "required": true,
                "unique": true
            }
        },
        {
            "type": "String",
            "id": "a2436179d3b8b64580f9c4e259251b8c",
            "name": "TXT",
            "title": "TXT",
            "description": "Nom court de la prescription servant à faire une étiquette",
            "constraints": {
                "required": false,
                "unique": false,
                "maxLength": 10
            }
        },
        {
            "type": "String",
            "id": "aa148732ae950845691ef5cae4af9833",
            "name": "LIB_IDPSC",
            "title": "LIB_IDPSC",
            "description": "identifiant d'objets",
            "constraints": {
                "required": false,
                "unique": false,
                "pattern": "[AB0-9]{5}_PSMV_[0-9]{8}_PS[0-9]+",
                "maxLength": 40
            }
        },
        {
            "type": "Url",
            "id": "aeb95306296d54a34057c6b2028fe7f2",
            "name": "URLFIC",
            "title": "URLFIC",
            "description": "URL ou URI qui pointe sur le fichier de règlement de la prescription",
            "constraints": {
                "required": false,
                "unique": false,
                "maxLength": 254
            }
        },
        {
            "type": "String",
            "id": "befc0bae8298a65bb261eb44ac8465ca",
            "name": "TYPEPSC",
            "title": "TYPEPSC",
            "description": "Type de la prescription dans une liste prédéfinie",
            "constraints": {
                "required": true,
                "unique": false,
                "maxLength": 2
            }
        },
        {
            "type": "String",
            "id": "df677fb16fb54d1e34527eb994765877",
            "name": "NATURE",
            "title": "NATURE",
            "description": "NATURE",
            "constraints": {
                "required": false,
                "unique": false,
                "maxLength": 50
            }
        },
        {
            "type": "String",
            "id": "e1bac9711444b231cd01042f4bf12555",
            "name": "LIBELLE",
            "title": "LIBELLE",
            "description": "Intitulé complet de la prescription",
            "constraints": {
                "required": true,
                "unique": false,
                "maxLength": 254
            }
        }
    ],
    "constraints": {
        "conditions": [
            "TYPEPSC NOT LIKE '97' OR STYPEPSC NOT LIKE '00' OR NOMFIC IS NOT NULL"
        ],
        "foreignKeys": [
            "(TYPEPSC, STYPEPSC) REFERENCES PrescriptionPSMVType(TYPEPSC, STYPEPSC)"
        ]
    }
}