{
    "id": "fe8538681215912957dd084c354e7420",
    "name": "INFO_PCT",
    "title": "INFO_PCT",
    "description": "Table contenant les informations ponctuelle à reporter à titre d'information",
    "columns": [
        {
            "type": "Filename",
            "id": "4137c78decf6640ddf104b6f3a27fde5",
            "name": "NOMFIC",
            "title": "NOMFIC",
            "description": "Nom du fichier contenant le texte décrivant l'information reportée",
            "constraints": {
                "required": false,
                "unique": false,
                "maxLength": 80
            }
        },
        {
            "type": "String",
            "id": "4f7fa2932ee1a85ad55bbd522ee13d9b",
            "name": "LIBELLE",
            "title": "LIBELLE",
            "description": "Nom complet de l'information",
            "constraints": {
                "required": true,
                "unique": false,
                "maxLength": 254
            }
        },
        {
            "type": "String",
            "id": "6d2571c9126933b56371887d17e82672",
            "name": "IDURBA",
            "title": "IDURBA",
            "description": "Identifiant du document d'urbanisme",
            "constraints": {
                "required": true,
                "unique": false,
                "maxLength": 30
            }
        },
        {
            "type": "String",
            "id": "815cd200c9791f74f7ee9bc4ff64129e",
            "name": "TXT",
            "title": "TXT",
            "description": "Nom court de l'information servant à faire une étiquette",
            "constraints": {
                "required": false,
                "unique": false,
                "maxLength": 10
            }
        },
        {
            "type": "String",
            "id": "a524a242fbddff1f78947d0192643416",
            "name": "TYPEINF",
            "title": "TYPEINF",
            "description": "Type d'information ponctuelle dans une liste prédéfinie",
