                "required": true,
                "unique": false,
                "enum": [
                    "PSMV"
                ],
                "maxLength": 4
            }
        },
        {
            "type": "Url",
            "id": "0d7830c305e70057f505e19af501564e",
            "name": "URLPE",
            "title": "URLPE",
            "constraints": {
                "required": false,
                "unique": false,
                "maxLength": 254
