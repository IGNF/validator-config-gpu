            "name": "ETAT",
            "title": "ETAT",
            "description": "Etat juridique du document d'urbanisme",
            "constraints": {
                "required": true,
                "unique": false,
                "enum": [
                    "01",
                    "02",
                    "03",
                    "04",
                    "05",
                    "06",
                    "07",
                    "08",
                    "09"
                ],
                "maxLength": 2
            }
        },
        {
            "type": "String",
            "id": "6999585510bd6659e2200ebb396f6a24",
            "name": "TYPEREF",
            "title": "TYPEREF",
            "description": "Nom du référentiel cadastral utilisé pour la numérisation",
            "constraints": {
                "required": false,
                "unique": false,
                "enum": [
                    "01",
                    "02",
                    "03",
                    "04",
                    "05"
                ],
                "maxLength": 2
            }
        },
        {
            "type": "String",
            "id": "6cca2750e3fbfa678e51bd4da8856392",
            "name": "IDURBA",
            "title": "IDURBA",
            "description": "Identifiant du document d'urbanisme",
            "constraints": {
                "required": true,
                "unique": false,
                "maxLength": 30
            }
        },
        {
            "type": "String",
            "id": "7eaace065f098ccdb8ff368fe2cbb5fc",
            "name": "DATAPPRO",
            "title": "DATAPPRO",
            "description": "Date de la dernière approbation administrative du document d'urbanisme",
            "constraints": {
                "required": false,
                "unique": false,
                "pattern": "[0-9]{8}"
            }
        },
        {
            "type": "String",
            "id": "8ad64b11a01bb49e6038c125c75b23d3",
            "name": "SIREN",
            "title": "SIREN",
            "description": "Indique le numéro SIREN de l'intercommunalité ayant approuvé le PLU. Clé externe permettant de faire le lien avec la table des EPCI",
            "constraints": {
                "required": false,
                "unique": false,
                "maxLength": 9
            }
        },
        {
            "type": "String",
            "id": "9e929b0259fc13ec8424164f4d3ea898",
            "name": "SITEWEB",
            "title": "SITEWEB",
            "description": "Page web du service de consultation du PLU offert par la collectivité locale",
            "constraints": {
                "required": false,
                "unique": false,
                "maxLength": 254
            }
        },
        {
            "type": "Url",
            "id": "a5e2a7024f6f47b8fd6cf6faf5377dd1",
            "name": "URLRAPP",
            "title": "URLRAPP",
            "description": "Lien d'accès au fichier contenant le dernier rapport de présentation",
            "constraints": {
                "required": false,
                "unique": false,
                "maxLength": 254
            }
        },
        {
            "type": "Url",
            "id": "a717aaf4bbca1427afb46d451cb71405",
            "name": "URLPLAN",
            "title": "URLPLAN",
            "description": "URL ou URI qui pointe sur le fichier du plan scanné",
            "constraints": {
                "required": false,
                "unique": false,
                "maxLength": 254
            }
        },
        {
            "type": "Url",
            "id": "af01d01546fcface8dffcf5413bd4f22",
            "name": "URLREG",
            "title": "URLREG",
            "description": "URL ou URI qui pointe sur le fichier du règlement papier complet scanné",
            "constraints": {
                "required": false,
                "unique": false,
                "maxLength": 254
            }
        },
        {
            "type": "String",
            "id": "c7f5eb1b9970b5b8fcb5fd933a5b3935",
            "name": "DATEREF",
            "title": "DATEREF",
            "description": "Date d'actualité ou de mise à jour du référentiel cadastral utilisé pour la création ou la numérisation des documents graphiques.",
            "constraints": {
                "required": true,
                "unique": false,
                "pattern": "[0-9]{8}"
            }
        },
        {
            "type": "String",
            "id": "d009e5eb90b887d4eee7654990920f54",
            "name": "NOMPROC",
            "title": "NOMPROC",
            "description": "Nom de la dernière procédure",
            "constraints": {
                "required": true,
                "unique": false,
                "pattern": "(E|R|A|MEC[0-9]+|MAJ[0-9]+|M[0-9]+|MS[0-9]+|RS[0-9]+|RA[0-9]+)",
                "maxLength": 10
            }
        },
        {
            "type": "String",
            "id": "deb3347e81eaca6ef1f677b0ee2897d6",
            "name": "NOMPLAN",
            "title": "NOMPLAN",
            "description": "Nom du fichier du règlement graphique ou de son tableau d'assemblage",
            "constraints": {
                "required": false,
                "unique": false,
                "pattern": "([0-9]{9}|(2A|2B|[0-9]{2})[0-9]{3})_reglement_graphique(_[0-9]+)?_[0-9]{8}(_[a-zA-Z])?(.pdf|.zip)",
                "maxLength": 80
            }
        }
    ],
    "constraints": {
        "conditions": [
            "DATAPPRO IS NOT NULL OR ETAT NOT LIKE '03'",
            "DATEFIN IS NOT NULL OR ETAT NOT IN ('04', '05')",
            "NOMREG IS NOT NULL OR URLREG IS NOT NULL",
            "NOMRAPP IS NOT NULL OR URLRAPP IS NOT NULL"
        ],
        "foreignKeys": [
            "(ETAT) REFERENCES ListeEtatPLU(ETAT)",
            "(TYPEDOC) REFERENCES ListeTypedocPLU(TYPEDOC)",
            "(TYPEREF) REFERENCES ListeTyperefPLU(TYPEREF)"
        ]
    }
}