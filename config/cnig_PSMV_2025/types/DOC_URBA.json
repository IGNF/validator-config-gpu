            "constraints": {
                "required": false,
                "unique": false,
                "maxLength": 80
            }
        },
        {
            "type": "String",
            "id": "30b5b45c9718870e0fa385794fa495c0",
            "name": "NOMREG",
            "title": "NOMREG",
            "description": "Nom du fichier contentant le document complet du règlement",
            "constraints": {
                "required": false,
                "unique": false,
                "pattern": "([0-9]{9}|(2A|2B|[0-9]{2})[0-9]{3})_reglement_[0-9]{8}.pdf",
