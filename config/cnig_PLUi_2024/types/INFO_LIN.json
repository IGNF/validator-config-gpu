    "id": "a36f2cb33fe4e74f5e94f092d8f1724b",
    "name": "INFO_LIN",
    "title": "INFO_LIN",
    "description": "Table contenant les informations linéaires à reporter à titre d'information",
    "constraints": {
        "conditions": [
            "NOMFIC IS NOT NULL OR TYPEINF NOT LIKE '97'"
