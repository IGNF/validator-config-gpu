            "type": "String",
            "constraints": {
                "required": false,
                "unique": false,
                "pattern": "([0-9]{9}|(2A|2B|[0-9]{2})[0-9]{3})_sectorisation_graphique(_[0-9]+)?_[0-9]{8}(_[a-zA-Z])?(.pdf|.zip)",
                "maxLength": 80
            }
        },
        {
            "id": "3a6262802a6f4e0a5008e31094485cdc",
            "name": "SIREN",
            "title": "SIREN",
