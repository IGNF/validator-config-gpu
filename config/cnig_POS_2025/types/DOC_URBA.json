{
    "id": "48689d87c1f7adacf0397c06349c5445",
    "name": "DOC_URBA",
    "title": "DOC_URBA",
    "description": "Table contenant la liste des documents d'Urbanisme PLU ou POS dont la numérisation ou l'élaboration sous fourme numérique est engagée",
    "columns": [
        {
            "type": "String",
            "id": "0722dd3c3e7a549994aef16472b75707",
            "name": "TYPEDOC",
            "title": "TYPEDOC",
            "description": "Type du document",
            "constraints": {
                "required": true,
                "unique": false,
                "enum": [
                    "POS"
                ],
                "maxLength": 4
            }
        },
        {
            "type": "Url",
            "id": "1ae3c2800c348502db71bc6361785e66",
            "name": "URLRAPP",
            "title": "URLRAPP",
            "description": "Lien d'accès au fichier contenant le dernier rapport de présentation",
            "constraints": {
                "required": false,
                "unique": false,
                "maxLength": 254
            }
        },
        {
            "type": "String",
            "id": "1d0c8025939453d2fc6b1fc95dd7e5b7",
            "name": "TYPEREF",
            "title": "TYPEREF",
            "description": "Nom du référentiel cadastral utilisé pour la numérisation",
            "constraints": {
                "required": false,
                "unique": false,
                "enum": [
                    "01",
                    "02",
                    "03",
                    "04",
                    "05"
                ],
                "maxLength": 2
            }
        },
        {
            "type": "String",
            "id": "301ec1b62132eb874bc4e08f28064f09",
            "name": "DATEFIN",
            "title": "DATEFIN",
            "description": "Date de fin de validité du document d'urbanisme",
            "constraints": {
                "required": false,
                "unique": false,
                "pattern": "[0-9]{8}"
            }
        },
        {
            "type": "String",
            "id": "58406b748db90a91e5248ecb53c0d871",
            "name": "DATAPPRO",
            "title": "DATAPPRO",
            "description": "Date de la dernière approbation administrative du document d'urbanisme",
            "constraints": {
                "required": false,
                "unique": false,
                "pattern": "[0-9]{8}"
            }
        },
        {
            "type": "String",
            "id": "5983ce5c2196c56223fd57028eaa2b51",
            "name": "NOMREG",
            "title": "NOMREG",
            "description": "Nom du fichier contentant le document complet du règlement",
            "constraints": {
                "required": false,
                "unique": false,
                "pattern": "([0-9]{9}|(2A|2B|[0-9]{2})[0-9]{3})_reglement_[0-9]{8}.pdf",
                "maxLength": 80
            }
        },
        {
            "type": "String",
            "id": "62e57cb57d26d3b25da0f7d5618e8eaf",
            "name": "SITEWEB",
            "title": "SITEWEB",
            "description": "Page web du service de consultation du PLU offert par la collectivité locale",
            "constraints": {
                "required": false,
                "unique": false,
                "maxLength": 254
            }
        },
        {
            "type": "Url",
            "id": "732f5f5dcb3af62df8e60b732a889ec5",
            "name": "URLPLAN",
            "title": "URLPLAN",
            "description": "URL ou URI qui pointe sur le fichier du plan scanné",
            "constraints": {
                "required": false,
                "unique": false,
                "maxLength": 254
            }
        },
        {
            "type": "String",
            "id": "7b5b51c2c6b97584b96491ed73e60b39",
            "name": "NOMPLAN",
            "title": "NOMPLAN",
            "description": "Nom du fichier du règlement graphique ou de son tableau d'assemblage",
            "constraints": {
                "required": false,
                "unique": false,
                "pattern": "([0-9]{9}|(2A|2B|[0-9]{2})[0-9]{3})_reglement_graphique(_[0-9]+)?_[0-9]{8}(_[a-zA-Z])?(.pdf|.zip)",
                "maxLength": 80
            }
        },
        {
            "type": "String",
            "id": "7bf93a65a00a1dd8bffb1f4139a61c8d",
            "name": "IDURBA",
            "title": "IDURBA",
            "description": "Identifiant du document d'urbanisme",
            "constraints": {
                "required": true,
                "unique": false,
                "maxLength": 30
            }
        },
        {
            "type": "String",
            "id": "8049b6e6ab519bb28d5bb54c9db9c8cd",
            "name": "SIREN",
            "title": "SIREN",
            "description": "Indique le numéro SIREN de l'intercommunalité ayant approuvé le PLU. Clé externe permettant de faire le lien avec la table des EPCI",
            "constraints": {
                "required": false,
                "unique": false,
                "maxLength": 9
            }
        },
        {
            "type": "String",
            "id": "b4d359a27ab7ed179d1ca1d6647ad999",
            "name": "NOMRAPP",
            "title": "NOMRAPP",
            "description": "Nom du ficher contenant le dernier rapport de présentation",
            "constraints": {
                "required": false,
                "unique": false,
                "maxLength": 80
            }
        },
        {
            "type": "String",
            "id": "d102401fbf4c9356b611325b2078d023",
            "name": "DATEREF",
            "title": "DATEREF",
            "description": "Date d'actualité ou de mise à jour du référentiel cadastral utilisé pour la création ou la numérisation des documents graphiques.",
            "constraints": {
                "required": true,
                "unique": false,
                "pattern": "[0-9]{8}"
            }
        },
        {
            "type": "Url",
            "id": "e49fefb7ede0a25efc065f8de9791d68",
            "name": "URLPE",
            "title": "URLPE",
            "constraints": {
                "required": false,
                "unique": false,
                "maxLength": 254
            }
        },
        {
            "type": "Url",
            "id": "e90d073137b791c66ad4c00a4eff1700",
            "name": "URLREG",
            "title": "URLREG",
            "description": "URL ou URI qui pointe sur le fichier du règlement papier complet scanné",
            "constraints": {
                "required": false,
                "unique": false,
                "maxLength": 254
            }
        },
        {
            "type": "String",
            "id": "ed4f0f25d45b2ecfa47203ec42457019",
            "name": "NOMPROC",
            "title": "NOMPROC",
            "description": "Nom de la dernière procédure",
            "constraints": {
                "required": true,
                "unique": false,
                "pattern": "(E|R|A|MEC[0-9]+|MAJ[0-9]+|M[0-9]+|MS[0-9]+|RS[0-9]+|RA[0-9]+)",
                "maxLength": 10
            }
        },
        {
            "type": "String",
            "id": "f948c6732ae7a78eecd84ef4060d59f8",
            "name": "ETAT",
            "title": "ETAT",
            "description": "Etat juridique du document d'urbanisme",
            "constraints": {
                "required": true,
                "unique": false,
                "enum": [
                    "01",
                    "02",
                    "03",
                    "04",
                    "05",
                    "06",
                    "07",
                    "08",
                    "09"
                ],
                "maxLength": 2
            }
        }
    ],
    "constraints": {
        "conditions": [
            "DATAPPRO IS NOT NULL OR ETAT NOT LIKE '03'",
            "DATEFIN IS NOT NULL OR ETAT NOT IN ('04', '05')",
            "NOMREG IS NOT NULL OR URLREG IS NOT NULL",
            "NOMRAPP IS NOT NULL OR URLRAPP IS NOT NULL"
        ],
        "foreignKeys": [
            "(ETAT) REFERENCES ListeEtatPLU(ETAT)",
            "(TYPEDOC) REFERENCES ListeTypedocPLU(TYPEDOC)",
            "(TYPEREF) REFERENCES ListeTyperefPLU(TYPEREF)"
        ]
    }
}