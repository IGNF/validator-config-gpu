{
    "id": "4467d4a214ed4a28a40c6c166bc780a8",
    "name": "INFO_PCT",
    "title": "INFO_PCT",
    "description": "Table contenant les informations ponctuelle à reporter à titre d'information",
    "columns": [
        {
            "id": "02e923e221f761c4273b0857df7f0404",
            "name": "TXT",
            "title": "TXT",
            "description": "Nom court de l'information servant à faire une étiquette",
            "type": "String",
            "constraints": {
                "required": false,
                "unique": false,
                "maxLength": 10
            }
        },
        {
            "id": "499df82e222b20513a87a46ba1c653fa",
            "name": "WKT",
            "title": "Géométrie",
            "description": "Geométrie associée",
            "type": "MultiPoint",
            "constraints": {
                "required": true,
                "unique": false
            }
        },
        {
            "id": "6a6391f8df42060dc43b959b99a802af",
            "name": "INSEE",
            "title": "INSEE",
            "description": "Code INSEE de la commune concernée par l'information",
            "type": "String",
            "constraints": {
                "required": true,
                "unique": false,
                "maxLength": 5
            }
        },
        {
            "id": "ae4ca2b37d5c96bb65c7a76cb8dbcecf",
            "name": "URLFIC",
            "title": "URLFIC",
            "description": "URL ou URI qui pointe sur le fichier de règlement de l'information",
            "type": "Url",
            "constraints": {
                "required": false,
                "unique": false,
                "maxLength": 254
            }
        },
        {
            "id": "b317462d1fc1ea40c668d8060a0578c1",
            "name": "IDINFO",
            "title": "IDINFO",
            "description": "Identifiant unique de l’objet",
            "type": "String",
            "constraints": {
                "required": false,
                "unique": false,
                "pattern": "POIN([0-9]+)?",
                "maxLength": 10
            }
        },
        {
            "id": "ba02ef06eb5e09a5ad02690bfb56daf7",
            "name": "LIBELLE",
            "title": "LIBELLE",
            "description": "Nom complet de l'information",
            "type": "String",
            "constraints": {
                "required": true,
                "unique": false,
                "maxLength": 254
            }
        },
        {
            "id": "d89be5964caf8137442128a228f79985",
            "name": "TYPEINF",
            "title": "TYPEINF",
            "description": "Type d'information ponctuelle dans une liste prédéfinie",
            "type": "String",
            "constraints": {
                "required": true,
                "unique": false,
                "enum": [
                    "16",
                    "19",
                    "27",
                    "99"
                ],
                "maxLength": 2
            }
        },
        {
            "id": "f7bad39ca64ddb8fc3da7ec232ade1bf",
            "name": "NOMFIC",
            "title": "NOMFIC",
            "description": "Nom du fichier contenant le texte décrivant l'information reportée",
            "type": "Filename",
            "constraints": {
                "required": false,
                "unique": false,
                "maxLength": 80
            }
        }
    ]
}