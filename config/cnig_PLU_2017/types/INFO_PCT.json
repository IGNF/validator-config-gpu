{
    "id": "e146c70132bfacbce05ea5b20dccdcd9",
    "name": "INFO_PCT",
    "title": "INFO_PCT",
    "description": "Table contenant les informations ponctuelle à reporter à titre d'information",
    "columns": [
        {
            "type": "Url",
            "id": "3730fe22440be9644671b129e0164842",
            "name": "URLFIC",
            "title": "URLFIC",
            "description": "URL ou URI qui pointe sur le fichier de règlement de l'information",
            "constraints": {
                "required": false,
                "presenceRequired": false,
                "unique": false,
                "maxLength": 254
            }
        },
        {
            "type": "String",
            "id": "7dc1e07efcce0435b1c61775ba23bc60",
            "name": "LIB_IDINFO",
            "title": "LIB_IDINFO",
            "description": "identifiant d'objets",
            "constraints": {
                "required": false,
                "presenceRequired": true,
                "unique": false,
                "pattern": "[AB0-9]{5}_PLU_[0-9]{8}_IP[0-9]+",
                "maxLength": 40
            }
        },
        {
            "type": "String",
            "id": "8ac6bb3ce1c8c73c6fd025f1c028516c",
            "name": "IDURBA",
            "title": "IDURBA",
            "description": "Identifiant du document d'urbanisme",
            "constraints": {
                "required": true,
                "presenceRequired": false,
                "unique": false,
                "maxLength": 30
            }
        },
        {
            "type": "MultiPoint",
            "id": "938feeac6e76d292950281830fd62ba6",
            "name": "WKT",
            "title": "Géométrie",
            "description": "Géométrie associée",
            "constraints": {
                "required": true,
                "presenceRequired": false,
                "unique": true
            }
        },
        {
            "type": "String",
            "id": "9a01d0d3477a210d16b9fbac99b30498",
            "name": "DATVALID",
            "title": "DATVALID",
            "constraints": {
                "required": false,
                "presenceRequired": false,
                "unique": false,
                "pattern": "[0-9]{8}"
            }
        },
        {
            "type": "String",
            "id": "cca364f8a7d623f95231aab602f903b3",
            "name": "LIBELLE",
            "title": "LIBELLE",
            "description": "Nom complet de l'information",
            "constraints": {
                "required": true,
                "presenceRequired": false,
                "unique": false,
                "maxLength": 254
            }
        },
        {
            "type": "Filename",
            "id": "d7c5dc3504eada80d726903da9bd2c75",
            "name": "NOMFIC",
            "title": "NOMFIC",
            "description": "Nom du fichier contenant le texte décrivant l'information reportée",
            "constraints": {
                "required": false,
                "presenceRequired": false,
                "unique": false,
                "maxLength": 80
            }
        },
        {
            "type": "String",
            "id": "dcf04b69c9c665a1843d13bb5528d668",
            "name": "TXT",
            "title": "TXT",
            "description": "Nom court de l'information servant à faire une étiquette",
            "constraints": {
                "required": false,
                "presenceRequired": false,
                "unique": false,
                "maxLength": 10
            }
        },
        {
            "type": "String",
            "id": "ef4f5f91752d87925d240364756a66bb",
            "name": "TYPEINF",
            "title": "TYPEINF",
            "description": "Type d'information ponctuelle dans une liste prédéfinie",
            "constraints": {
                "required": true,
                "presenceRequired": false,
                "unique": false,
                "maxLength": 2
            }
        },
        {
            "type": "String",
            "id": "f68bd8254c413955a1a330b98e85cd69",
            "name": "STYPEINF",
            "title": "STYPEINF",
            "constraints": {
                "required": true,
                "presenceRequired": false,
                "unique": false,
                "maxLength": 2
            }
        }
    ],
    "constraints": {
        "conditions": [
            "NOMFIC IS NOT NULL OR TYPEINF NOT LIKE '97'"
        ],
        "foreignKeys": [
            "(TYPEINF, STYPEINF) REFERENCES InformationUrbaType(TYPEINF, STYPEINF)"
        ]
    }
}