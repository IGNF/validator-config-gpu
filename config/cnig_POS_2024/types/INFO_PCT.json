{
    "id": "372af283c345b8c0ba148582f1c3c99e",
    "name": "INFO_PCT",
    "title": "INFO_PCT",
    "description": "Table contenant les informations ponctuelle à reporter à titre d'information",
    "columns": [
        {
            "type": "String",
            "id": "05ede2f1485ccb847f97d47d0d2c2633",
            "name": "IDURBA",
            "title": "IDURBA",
            "description": "Identifiant du document d'urbanisme",
            "constraints": {
                "required": true,
                "unique": false,
                "maxLength": 30
            }
        },
        {
            "type": "Url",
            "id": "132c902f08ff9f23c0f52661f21cba9a",
            "name": "URLFIC",
            "title": "URLFIC",
            "description": "URL ou URI qui pointe sur le fichier de règlement de l'information",
            "constraints": {
                "required": false,
                "unique": false,
                "maxLength": 254
            }
        },
        {
            "type": "String",
            "id": "1c1b2e0a7f3636e4eeaf9644ecf0a2d8",
            "name": "LIBELLE",
            "title": "LIBELLE",
            "description": "Nom complet de l'information",
            "constraints": {
                "required": true,
                "unique": false,
                "maxLength": 254
            }
        },
        {
            "type": "MultiPoint",
            "id": "258fbfec39d8a5594b7b63425dcd1674",
            "name": "WKT",
            "title": "Géométrie",
            "description": "Géométrie associée",
            "constraints": {
                "required": true,
                "unique": true
            }
        },
        {
            "type": "String",
            "id": "3dedd5e2ca218416e33ed2ee5ef916b3",
            "name": "SYMBOLE",
            "title": "SYMBOLE",
            "description": "SYMBOLE",
            "constraints": {
                "required": false,
                "unique": false,
                "pattern": "INF-P_[0-9]{2}-[0-9]{2}_[0-9]{3}",
                "maxLength": 20
            }
        },
        {
            "type": "String",
            "id": "4294d6f209e63cbdd4bf5d4d9e96eb87",
            "name": "STYPEINF",
            "title": "STYPEINF",
            "constraints": {
                "required": true,
                "unique": false,
                "maxLength": 2,
                "reference": "ListeStypeinf(STYPEINF)"
            }
        },
        {
            "type": "String",
            "id": "4c9566a9ce24315024cb9b1b85c70836",
            "name": "TYPEINF",
            "title": "TYPEINF",
            "description": "Type d'information ponctuelle dans une liste prédéfinie",
            "constraints": {
                "required": true,
                "unique": false,
                "maxLength": 2
            }
        },
        {
            "type": "String",
            "id": "50907705a11456e3da36f81476d5e524",
            "name": "DATVALID",
            "title": "DATVALID",
            "constraints": {
                "required": false,
                "unique": false,
                "pattern": "[0-9]{8}"
            }
        },
        {
            "type": "Filename",
            "id": "ad9d17a37756680a471fd4221ac806cc",
            "name": "NOMFIC",
            "title": "NOMFIC",
            "description": "Nom du fichier contenant le texte décrivant l'information reportée",
            "constraints": {
                "required": false,
                "unique": false,
                "maxLength": 80
            }
        },
        {
            "type": "String",
            "id": "d0fa6b19641a813b200280a1b37e1a27",
            "name": "LIB_IDINFO",
            "title": "LIB_IDINFO",
            "description": "identifiant d'objets",
            "constraints": {
                "required": false,
                "unique": false,
                "pattern": "[AB0-9]{5}_POS_[0-9]{8}_IP[0-9]+",
                "maxLength": 40
            }
        },
        {
            "type": "String",
            "id": "e3e7516fbcf729ed47d02731bc3db655",
            "name": "TXT",
            "title": "TXT",
            "description": "Nom court de l'information servant à faire une étiquette",
            "constraints": {
                "required": false,
                "unique": false,
                "maxLength": 10
            }
        }
    ],
    "constraints": {
        "conditions": [
            "NOMFIC IS NOT NULL OR TYPEINF NOT LIKE '97'"
        ],
        "foreignKeys": [
            "(TYPEINF, STYPEINF) REFERENCES InformationUrbaType(TYPEINF, STYPEINF)",
            "(STYPEINF) REFERENCES ListeStypeinf(STYPEINF)"
        ]
    }
}