    "name": "INFO_PCT",
    "title": "INFO_PCT",
    "description": "Table contenant les informations ponctuelle à reporter à titre d'information",
    "columns": [
        {
            "type": "String",
            "id": "0a6704516310aee772f1ea248dedddde",
            "name": "TXT",
            "title": "TXT",
            "description": "Nom court de l'information servant à faire une étiquette",
            "constraints": {
                "required": false,
                "unique": false,
                "maxLength": 10
            }
        },
        {
            "type": "Url",
            "id": "524fc5cbe5215fce826bdae18b6ff376",
