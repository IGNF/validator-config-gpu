            "constraints": {
                "required": true,
                "unique": false,
                "maxLength": 30
            }
        },
        {
            "type": "String",
            "id": "2f5999f8939d23dabd7d22b10c661048",
            "name": "NOMPROC",
            "title": "NOMPROC",
            "description": "Nom de la dernière procédure",
            "constraints": {
                "required": true,
                "unique": false,
                "pattern": "(E|R|A|MEC[0-9]+|MAJ[0-9]+|M[0-9]+|MS[0-9]+|RS[0-9]+|RA[0-9]+)",
                "maxLength": 10
            }
        },
        {
            "type": "String",
            "id": "33f7176cde52156f1706fd148f822bec",
            "name": "NOMREG",
            "title": "NOMREG",
            "description": "Nom du fichier contentant le document complet du règlement",
            "constraints": {
                "required": false,
                "unique": false,
                "pattern": "([0-9]{9}|(2A|2B|[0-9]{2})[0-9]{3})_reglement_[0-9]{8}.pdf",
                "maxLength": 80
            }
        },
        {
            "type": "String",
            "id": "3979804ac81d4e443c8e55265bb24b94",
            "name": "DATEFIN",
            "title": "DATEFIN",
            "description": "Date de fin de validité du document d'urbanisme",
            "constraints": {
                "required": false,
                "unique": false,
                "pattern": "[0-9]{8}"
            }
        },
        {
            "type": "String",
            "id": "3cc7fa12918d4d6be2e131ab00cefdac",
            "name": "TYPEDOC",
            "title": "TYPEDOC",
            "description": "Type du document",
            "constraints": {
                "required": true,
                "unique": false,
                "enum": [
                    "PLU",
                    "POS",
                    "CC",
                    "PSMV"
                ],
                "maxLength": 4
            }
        },
        {
            "type": "String",
            "id": "4e094d31476df252a2bd5064107a7783",
            "name": "DATEREF",
            "title": "DATEREF",
            "description": "Date d'actualité ou de mise à jour du référentiel cadastral utilisé pour la création ou la numérisation des documents graphiques.",
            "constraints": {
                "required": true,
                "unique": false,
                "pattern": "[0-9]{8}"
            }
        },
        {
            "type": "String",
            "id": "5a9a15b4854baed306fee328ac954bc5",
            "name": "ETAT",
            "title": "ETAT",
            "description": "Etat juridique du document d'urbanisme",
            "constraints": {
                "required": true,
                "unique": false,
                "enum": [
                    "01",
                    "02",
                    "03",
                    "04",
                    "05",
                    "06",
                    "07",
                    "08",
                    "09"
                ],
                "maxLength": 2
            }
        },
        {
            "type": "String",
            "id": "5d2279a4b866dcb5f9f0d2474a063538",
            "name": "DATAPPRO",
            "title": "DATAPPRO",
            "description": "Date de la dernière approbation administrative du document d'urbanisme",
            "constraints": {
                "required": false,
                "unique": false,
                "pattern": "[0-9]{8}"
            }
        },
        {
            "type": "String",
            "id": "652ed84f93be3e3d6f35dea81a292f41",
            "name": "SITEWEB",
            "title": "SITEWEB",
            "description": "Page web du service de consultation du PLU offert par la collectivité locale",
            "constraints": {
                "required": false,
                "unique": false,
                "maxLength": 254
            }
        },
        {
            "type": "String",
            "id": "9cb2e44c63876640c7b4d2bf52128f47",
            "name": "NOMRAPP",
            "title": "NOMRAPP",
            "description": "Nom du ficher contenant le dernier rapport de présentation",
            "constraints": {
                "required": false,
                "unique": false,
                "maxLength": 80
            }
        },
        {
            "type": "String",
            "id": "b8bc45916cdb3f03cb4fa36b4effc18f",
            "name": "NOMPLAN",
            "title": "NOMPLAN",
            "description": "Nom du fichier du règlement graphique ou de son tableau d'assemblage",
            "constraints": {
                "required": false,
                "unique": false,
                "pattern": "([0-9]{9}|(2A|2B|[0-9]{2})[0-9]{3})_reglement_graphique(_[0-9]+)?_[0-9]{8}(_[a-zA-Z])?(.pdf|.zip)",
                "maxLength": 80
            }
        },
        {
            "type": "String",
            "id": "b9fe206a156e4c48a36299a222fd5c7d",
            "name": "SIREN",
            "title": "SIREN",
            "description": "Indique le numéro SIREN de l'intercommunalité ayant approuvé le PLU. Clé externe permettant de faire le lien avec la table des EPCI",
            "constraints": {
                "required": false,
                "unique": false,
                "maxLength": 9
            }
        },
        {
            "type": "Url",
            "id": "d13542d1f3b7db10d8a4a0028644c7e5",
            "name": "URLPLAN",
            "title": "URLPLAN",
            "description": "URL ou URI qui pointe sur le fichier du plan scanné",
            "constraints": {
                "required": false,
                "unique": false,
                "maxLength": 254
            }
        },
        {
            "type": "String",
            "id": "f1189ccd72cedf88c22d5beaef44b680",
            "name": "TYPEREF",
            "title": "TYPEREF",
            "description": "Nom du référentiel cadastral utilisé pour la numérisation",
            "constraints": {
                "required": false,
                "unique": false,
                "enum": [
                    "01",
                    "02",
                    "03",
                    "04",
                    "05"
                ],
                "maxLength": 2
            }
        },
        {
            "type": "Url",
            "id": "fdc4ddf529d20293ae1f199743a6ac7f",
            "name": "URLRAPP",
            "title": "URLRAPP",
            "description": "Lien d'accès au fichier contenant le dernier rapport de présentation",
            "constraints": {
                "required": false,
                "unique": false,
                "maxLength": 254
            }
        }
    ],
    "constraints": {
        "conditions": [
            "DATAPPRO IS NOT NULL OR ETAT NOT LIKE '03'",
            "DATEFIN IS NOT NULL OR ETAT NOT IN ('04', '05')",
            "NOMREG IS NOT NULL OR URLREG IS NOT NULL",
            "NOMRAPP IS NOT NULL OR URLRAPP IS NOT NULL"
        ],
        "foreignKeys": [
            "(ETAT) REFERENCES ListeEtatPLU(ETAT)",
            "(TYPEDOC) REFERENCES ListeTypedocPLU(TYPEDOC)",
            "(TYPEREF) REFERENCES ListeTyperefPLU(TYPEREF)"
        ]
    }
}