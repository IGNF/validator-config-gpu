{
    "id": "c251a1eb9170b0e85c672c0b53dfa244",
    "name": "DOC_URBA",
    "title": "DOC_URBA",
    "description": "Table contenant la liste des documents d'Urbanisme PLU ou POS dont la numérisation ou l'élaboration sous fourme numérique est engagée",
    "columns": [
        {
            "type": "Url",
            "id": "17bad748b0bb4361455ac976ac41c13b",
            "name": "URLREG",
            "title": "URLREG",
            "description": "URL ou URI qui pointe sur le fichier du règlement papier complet scanné",
            "constraints": {
                "required": false,
                "unique": false,
                "maxLength": 254
            }
        },
        {
            "type": "Url",
            "id": "2a3a35fca4ccba65f51d5aa2b3dca30a",
            "name": "URLPE",
            "title": "URLPE",
            "constraints": {
                "required": false,
                "unique": false,
                "maxLength": 254
            }
        },
        {
            "type": "String",
            "id": "2b4aa5c251b4e3bb953c9a3875297b2b",
            "name": "IDURBA",
            "title": "IDURBA",
