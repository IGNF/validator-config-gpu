{
    "id": "cdc224f016595cc49d968f0c4a8af992",
    "name": "INFO_LIN",
    "title": "INFO_LIN",
    "description": "Table contenant les informations linéaires à reporter à titre d'information",
    "columns": [
        {
            "type": "String",
            "id": "1c37db058946a811594af48417596b9c",
            "name": "LIBELLE",
            "title": "LIBELLE",
            "description": "Nom complet de l'information",
            "constraints": {
                "required": true,
                "unique": false,
                "maxLength": 254
            }
        },
        {
            "type": "String",
            "id": "3eff82b65c648d49e8588e570d275970",
            "name": "STYPEINF",
            "title": "STYPEINF",
            "constraints": {
                "required": true,
                "unique": false,
                "maxLength": 2,
                "reference": "ListeStypeinf(STYPEINF)"
            }
        },
        {
            "type": "String",
            "id": "5f222065729483bf9a01281d627990d6",
            "name": "DATVALID",
            "title": "DATVALID",
            "constraints": {
                "required": false,
                "unique": false,
                "pattern": "[0-9]{8}"
            }
        },
        {
            "type": "String",
            "id": "893ddd805b6de56fae9169f878c1dacd",
            "name": "TYPEINF",
            "title": "TYPEINF",
            "description": "Type d'information linéaire dans une liste prédéfinie",
            "constraints": {
                "required": true,
                "unique": false,
                "maxLength": 2
            }
        },
        {
            "type": "String",
            "id": "8f3175924d5ea83b21d740d4ca0032f7",
            "name": "TXT",
            "title": "TXT",
            "description": "Nom court de l'information servant à faire une étiquette",
            "constraints": {
                "required": false,
                "unique": false,
                "maxLength": 10
            }
        },
        {
            "type": "Url",
            "id": "9036e39c50267888c817a7f2a14dbb9b",
            "name": "URLFIC",
            "title": "URLFIC",
            "description": "URL ou URI qui pointe sur le fichier de règlement de l'information",
            "constraints": {
                "required": false,
                "unique": false,
                "maxLength": 254
            }
        },
        {
            "type": "String",
            "id": "9373f7592ed1be9aca06ade75657b0a4",
            "name": "IDURBA",
            "title": "IDURBA",
            "description": "Identifiant du document d'urbanisme",
            "constraints": {
                "required": true,
                "unique": false,
                "maxLength": 30
            }
        },
        {
            "type": "Filename",
            "id": "a371f5d8adb7b2eca068093c447a1a64",
            "name": "NOMFIC",
            "title": "NOMFIC",
            "description": "Nom du fichier contenant le texte décrivant l'information reportée",
            "constraints": {
                "required": false,
                "unique": false,
                "maxLength": 80
            }
        },
        {
            "type": "String",
            "id": "a9ea8c0807639fb2e0851525fdef00f7",
            "name": "LIB_IDINFO",
            "title": "LIB_IDINFO",
            "description": "identifiant d'objets",
            "constraints": {
                "required": false,
                "unique": false,
                "pattern": "[AB0-9]{5}_PLU_[0-9]{8}_IL[0-9]+",
                "maxLength": 40
            }
        },
        {
            "type": "String",
            "id": "cbb43c19d0b3c6ff18d1288b0f69fdba",
            "name": "SYMBOLE",
            "title": "SYMBOLE",
            "description": "SYMBOLE",
            "constraints": {
                "required": false,
                "unique": false,
                "pattern": "INF-L_<ENTIER_2>-<ENTIER_2>_<ENTIER_3>",
                "maxLength": 20
            }
        },
        {
            "type": "MultiLineString",
            "id": "f54c13a66d035f10a95804ffa1d5fdbf",
            "name": "WKT",
            "title": "Géométrie",
            "description": "Géométrie associée",
            "constraints": {
                "required": true,
                "unique": true
            }
        }
    ],
    "constraints": {
        "conditions": [
            "NOMFIC IS NOT NULL OR TYPEINF NOT LIKE '97'"
        ],
        "foreignKeys": [
            "(TYPEINF, STYPEINF) REFERENCES InformationUrbaType(TYPEINF, STYPEINF)"
        ]
    }
}