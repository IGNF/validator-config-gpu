            "title": "SYMBOLE",
            "description": "SYMBOLE",
            "type": "String",
            "constraints": {
                "required": false,
                "unique": false,
                "pattern": "INF-L_[0-9]{2}-[0-9]{2}_[0-9]{3}",
                "maxLength": 20
            }
        },
        {
            "id": "77df6f852f860284bccb53d6e62765e4",
            "name": "TXT",
            "title": "TXT",
            "description": "Nom court de l'information servant à faire une étiquette",
            "type": "String",
            "constraints": {
                "required": false,
                "unique": false,
                "maxLength": 10
            }
        },
        {
            "id": "797ce7ab0b2f70db73ed3e85fea500af",
            "name": "NOMFIC",
            "title": "NOMFIC",
