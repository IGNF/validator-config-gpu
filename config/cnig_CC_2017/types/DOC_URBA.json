    "constraints": {
        "conditions": [
            "DATAPPRO IS NOT NULL OR ETAT NOT LIKE '03'",
            "DATEFIN IS NOT NULL OR ETAT NOT IN ('04', '05')"
        ],
        "foreignKeys": [
            "(ETAT) REFERENCES ListeEtatCC(ETAT)",
            "(TYPEDOC) REFERENCES ListeTypedocCC(TYPEDOC)",
            "(TYPEREF) REFERENCES ListeTyperefCC(TYPEREF)"
        ]
    }
}