            "constraints": {
                "required": false,
                "presenceRequired": false,
                "unique": false,
                "maxLength": 9
            }
        },
        {
            "type": "Url",
            "id": "ee7524de91886a3f18bce4f91a09599a",
            "name": "URLPLAN",
            "title": "URLPLAN",
            "description": "URL ou URI qui pointe sur le fichier du plan scanné",
            "constraints": {
                "required": false,
                "presenceRequired": false,
                "unique": false,
                "maxLength": 254
            }
        },
        {
            "type": "String",
            "id": "f71a99c56c4fc4a46b185b66e93b2c4f",
            "name": "DATAPPRO",
            "title": "DATAPPRO",
            "description": "Date de la dernière approbation administrative du document d'urbanisme",
            "constraints": {
                "required": false,
                "presenceRequired": false,
                "unique": false,
                "pattern": "[0-9]{8}"
            }
        }
