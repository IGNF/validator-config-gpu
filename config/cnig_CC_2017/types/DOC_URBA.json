            "description": "Nom du fichier du plan graphique",
            "constraints": {
                "required": false,
                "presenceRequired": false,
                "unique": false,
                "pattern": "([0-9]{9}|(2A|2B|[0-9]{2})[0-9]{3})_sectorisation_graphique(_[0-9]+)?_[0-9]{8}(_[a-zA-Z])?(.pdf|.zip)",
                "maxLength": 80
            }
