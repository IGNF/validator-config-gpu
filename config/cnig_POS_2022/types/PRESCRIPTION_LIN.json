{
    "id": "13dfa3bd0f7ba1c6f78ed3870b564192",
    "name": "PRESCRIPTION_LIN",
    "title": "PRESCRIPTION_LIN",
    "description": "Table contenant les prescriptions linéaires se superposant au document d'urbanisme et le lien avec le texte qui la décrit",
    "columns": [
        {
            "type": "String",
            "id": "0874d464ad1d2e51fb21711256b8dd5d",
            "name": "TYPEPSC",
            "title": "TYPEPSC",
            "description": "Type de la prescription dans une liste prédéfinie",
            "constraints": {
                "required": true,
                "unique": false,
                "maxLength": 2
            }
        },
        {
            "type": "Filename",
            "id": "19abdafa33801fd1485f0a9e9227575c",
            "name": "NOMFIC",
            "title": "NOMFIC",
            "description": "Nom du fichier PDF décrivant la prescription",
            "constraints": {
                "required": false,
                "unique": false,
                "maxLength": 80
            }
        },
        {
            "type": "Url",
            "id": "2aad64fa7913a286a2d46dfcfc83fcff",
            "name": "URLFIC",
            "title": "URLFIC",
            "description": "URL ou URI qui pointe sur le fichier de règlement de la prescription",
            "constraints": {
                "required": false,
                "unique": false,
                "maxLength": 254
            }
        },
        {
            "type": "String",
            "id": "2c3f171c1dc641a482227595a893a39e",
            "name": "NATURE",
            "title": "NATURE",
            "description": "NATURE",
            "constraints": {
                "required": false,
                "unique": false,
                "maxLength": 50
            }
        },
        {
            "type": "String",
            "id": "3962563a897424797bafca2b90ce70fd",
            "name": "TXT",
            "title": "TXT",
            "description": "Nom court de la prescription servant à faire une étiquette",
            "constraints": {
                "required": false,
                "unique": false,
                "maxLength": 10
            }
        },
        {
            "type": "String",
            "id": "470d2997b158954061a24a5edc3bb9f5",
            "name": "IDURBA",
            "title": "IDURBA",
            "description": "Identifiant du document d'urbanisme",
            "constraints": {
                "required": true,
                "unique": false,
                "maxLength": 30
            }
        },
        {
            "type": "String",
            "id": "485ba7f227493b97e221f02d10236937",
            "name": "LIBELLE",
            "title": "LIBELLE",
            "description": "Intitulé complet de la prescription",
            "constraints": {
                "required": true,
                "unique": false,
                "maxLength": 254
            }
        },
        {
            "type": "String",
            "id": "573fc2bc7b9d09675485f05141905592",
            "name": "SYMBOLE",
            "title": "SYMBOLE",
            "description": "SYMBOLE",
            "constraints": {
                "required": false,
                "unique": false,
                "pattern": "PSC-L_<ENTIER_2>-<ENTIER_2>_<ENTIER_3>",
                "maxLength": 20
            }
        },
        {
            "type": "MultiLineString",
            "id": "8a1692f7ee42f0d56908b5a40401cfa0",
            "name": "WKT",
            "title": "Géométrie",
            "description": "Géométrie associée",
            "constraints": {
                "required": true,
                "unique": true
            }
        },
        {
            "type": "String",
            "id": "d14756abbf052c449d2b4d8095c80266",
            "name": "DATVALID",
            "title": "DATVALID",
            "description": "Date de la dernière validation de la prescription",
            "constraints": {
                "required": false,
                "unique": false,
                "pattern": "[0-9]{8}"
            }
        },
        {
            "type": "String",
            "id": "d65a03e3f303e2524c33d363664a3025",
            "name": "LIB_IDPSC",
            "title": "LIB_IDPSC",
            "description": "identifiant d'objets",
            "constraints": {
                "required": false,
                "unique": false,
                "pattern": "[AB0-9]{5}_POS_[0-9]{8}_PL[0-9]+",
                "maxLength": 40
            }
        },
        {
            "type": "String",
            "id": "e8faeb02478997c05a1b64a518d7fcd1",
            "name": "STYPEPSC",
            "title": "STYPEPSC",
            "constraints": {
                "required": true,
                "unique": false,
                "maxLength": 2
            }
        }
    ],
    "constraints": {
        "conditions": [
            "TYPEPSC NOT LIKE '97' OR STYPEPSC NOT LIKE '00' OR NOMFIC IS NOT NULL"
        ],
        "foreignKeys": [
            "(TYPEPSC, STYPEPSC) REFERENCES PrescriptionUrbaType(TYPEPSC, STYPEPSC)"
        ]
    }
}