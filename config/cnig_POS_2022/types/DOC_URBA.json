            "constraints": {
                "required": false,
                "unique": false,
                "maxLength": 2
            }
        },
        {
            "type": "String",
            "id": "b582828525004267dc73fb542a569538",
            "name": "SIREN",
            "title": "SIREN",
            "description": "Indique le numéro SIREN de l'intercommunalité ayant approuvé le PLU. Clé externe permettant de faire le lien avec la table des EPCI",
            "constraints": {
                "required": false,
                "unique": false,
                "maxLength": 9
            }
        }
