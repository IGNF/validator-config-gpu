<?xml version="1.0" encoding="UTF-8" ?>
<document>
	<shortName>HABILLAGE_TXT</shortName>
	<description>Table des objets textuels (étiquettes) figurant sur le plan du document d'urbanisme à des fins d'ha</description>
	<attributes>
		<attribute>
			<name>NATECR</name>
			<description>Nature de l'écriture</description>
			<type>String</type>
			<size>40</size>
			<listOfValues />
			<nullable>true</nullable>
		</attribute>
		<attribute>
			<name>TXT</name>
			<description>Libellé de l'étiquette attachée au point d'habillage</description>
			<type>String</type>
			<size>80</size>
			<listOfValues />
			<nullable>true</nullable>
		</attribute>
		<attribute>
			<name>INSEE</name>
			<regexp>[0-9]{5}</regexp>
			<description>Code INSEE de la commune concernée par l'élément d'habillage</description>
			<type>String</type>
			<size>5</size>
			<listOfValues />
			<nullable>true</nullable>
		</attribute>
		<attribute>
			<name>WKT</name>
			<description>Geométrie associée</description>
			<type>MultiPoint</type>
			<size />
			<listOfValues />
			<crs>IGNF:LAMB93</crs>
			<nullable>false</nullable>
		</attribute>
	</attributes>
</document>