<?xml version="1.0" encoding="UTF-8" ?>
<document>
	<shortName>SERVITUDE</shortName>
	<description>Table contenant la liste des servitudes d'utilité publique</description>
	<attributes>
		<attribute>
			<name>IDSUP</name>
			<description>Identifiant de la SUP</description>
			<type>Integer</type>
			<size />
			<listOfValues />
			<nullable>false</nullable>
		</attribute>
		<attribute>
			<name>IDGEST</name>
			<description>Identifiant du gestionnaire de la SUP</description>
			<type>Integer</type>
			<size />
