<?xml version="1.0" encoding="UTF-8" ?>
<document>
	<shortName>PRESCRIPTION_SURF</shortName>
	<description>Table contenant les prescriptions surfaciques se superposant au document d'urbanisme et le lien avec le texte qui la décrit</description>
	<attributes>
		<attribute>
			<name>LIBELLE</name>
			<description>Intitulé complet de la prescription</description>
			<type>String</type>
			<size>254</size>
			<listOfValues />
			<nullable>false</nullable>
		</attribute>
		<attribute>
			<name>TXT</name>
			<description>Nom court de la prescription servant à faire une étiquette</description>
			<type>String</type>
			<size>10</size>
			<listOfValues />
			<nullable>true</nullable>
		</attribute>
		<attribute>
			<name>TYPEPSC</name>
			<description>Type de la prescription dans une liste prédéfinie</description>
			<type>String</type>
			<size>2</size>
			<listOfValues>
				<value>01</value>
				<value>02</value>
				<value>03</value>
				<value>04</value>
				<value>05</value>
				<value>06</value>
				<value>07</value>
				<value>08</value>
				<value>09</value>
				<value>10</value>
				<value>11</value>
				<value>12</value>
				<value>13</value>
				<value>14</value>
				<value>15</value>
				<value>16</value>
				<value>17</value>
				<value>18</value>
				<value>19</value>
				<value>20</value>
				<value>21</value>
				<value>22</value>
				<value>23</value>
				<value>24</value>
				<value>25</value>
				<value>26</value>
				<value>27</value>
				<value>29</value>
				<value>99</value>
			</listOfValues>
			<nullable>false</nullable>
		</attribute>
		<attribute>
			<name>NOMFIC</name>
			<description>Nom du fichier PDF décrivant la prescription</description>
			<type>String</type>
			<size>80</size>
			<listOfValues />
			<nullable>true</nullable>
		</attribute>
		<attribute>
			<name>URLFIC</name>
			<description>URL ou URI qui pointe sur le fichier de règlement de la prescription</description>
			<type>String</type>
			<size>254</size>
			<listOfValues />
			<nullable>true</nullable>
		</attribute>
		<attribute>
			<name>INSEE</name>
			<regexp>[0-9]{5}</regexp>
			<description>Code INSEE de la commune concernée par la prescription</description>
			<type>String</type>
			<size>5</size>
			<listOfValues />
			<nullable>true</nullable>
		</attribute>
		<attribute>
			<name>DATAPPRO</name>
			<description>Date de la dernière approbation de la prescription</description>
			<type>Date</type>
			<size>10</size>
			<listOfValues />
			<nullable>true</nullable>
		</attribute>
		<attribute>
			<name>DATVALID</name>
			<description>Date de la dernière validation de la prescription</description>
			<type>Date</type>
			<size>10</size>
			<listOfValues />
			<nullable>true</nullable>
		</attribute>
		<attribute>
			<name>WKT</name>
			<description>Geométrie associée</description>
			<type>MultiPolygon</type>
			<size />
			<listOfValues />
			<crs>IGNF:LAMB93</crs>
			<nullable>false</nullable>
		</attribute>
	</attributes>
</document>